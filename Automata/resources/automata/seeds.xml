					<state>Alive</state>
					<quantity>2</quantity>
					<direction></direction>
				</condition>
			</conditions>
		</transition>
		<transition>
			<state-from>Alive</state-from>
			<state-to>Dead</state-to>
			<conditions>
			</conditions>
		</transition>
	</transitions>
</automata>
